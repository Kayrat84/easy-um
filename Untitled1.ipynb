    "import mechanize\n",
    "import requests\n",
    "from bs4 import BeautifulSoup as bs\n",
    "from requests import Response\n",
    "dct = {\n",
    "    'СОГАЗ'     :'1000027835',\n",
    "    'МЕДСТРАХ'  :'1000027833',\n",
    "    'МАКС-М'    :'1000027832',\n",
    "    'КАПИТАЛ'   :'1000027837',\n",
    "    'ИНГОССТРАХ':'1000027836',\n",
    "    'РЕСО'      :'1000027834'\n",
    "}\n",
    "br = mechanize.Browser()\n",
    "br.set_handle_robots(False)\n",
    "br.open(\"https://www.mgfoms.ru/chastnye-lica/proverka-polisa\")\n",
    "def new(nom):\n",
    "    br.open(\"https://www.mgfoms.ru/chastnye-lica/proverka-polisa\")\n",
    "    br.select_form(nr=2)\n",
    "    br['number_new'] = nom\n",
    "    br.submit()\n",
    "    url = br.geturl()\n",
    "    page = requests.post(url)\n",
    "    soup = bs(page.text)\n",
    "    sk = soup.find_all(class_='field-content')\n",
    "    res=''\n",
    "    if sk != []:\n",
    "        for i in dct.keys():\n",
    "            if i in str(sk[0]):\n",
    "                res=dct[i]\n",
    "    return res\n",
    "\n",
    "def old(nom,ser):\n",
    "    br.open(\"https://www.mgfoms.ru/chastnye-lica/proverka-polisa\")\n",
    "    br.select_form(nr=1)\n",
    "    br['number_old'] = nom\n",
    "    br['range'] = ser\n",
    "    br.submit()\n",
    "    url = br.geturl()\n",
    "    page = requests.post(url)\n",
    "    soup = bs(page.text)\n",
    "    sk = soup.find_all(class_='field-content')\n",
    "    res=''\n",
    "    if sk != []:\n",
    "        for i in dct.keys():\n",
    "            if i in str(sk[0]):\n",
    "                res=dct[i]\n",
    "    return res\n",
    "\n",
    "def mo(ne, no):\n",
    "    dc = {\n",
    "        'СОГАЗ-МЕД': '1000027757',\n",
    "        'МЕДСТРАХ': '1000027756',\n",
    "        'МАКС-М': '1000027755',\n",
    "        'КАПИТАЛ': '1000027758',\n",
    "        'РЕСО-МЕД': '1000027754'\n",
    "    }\n",
    "\n",
    "    br.open(\"http://www.mofoms.ru/polis/\")\n",
    "    br.select_form(nr=0)\n",
    "    br['npolice'] = no  # i[2]\n",
    "    br['bth_date'] = ne  # i[3]\n",
    "    response = br.submit()\n",
    "    content = response.read()\n",
    "    soup = bs(content, \"html.parser\")\n",
    "    allNews = soup.find_all('div', class_=None)\n",
    "    st = allNews[4].text\n",
    "    res = ''\n",
    "    if 'Состоит на учёте' in st:\n",
    "        for j in dc.keys():\n",
    "            if j in st:\n",
    "                res = dc[j]\n",
    "    return res\n",
    "\n",
    "for a in row1:\n",
    "    if len(a) == 4:\n",
    "        if len(a[2]) == 16:\n",
    "            if a[2].startswith('770'):\n",
    "                a.append(old(a[2][6:],a[2][:6]))\n",
    "                if a[4] == '':\n",
    "                    a[4] = mo(a[3],a[2])\n",
    "            else:\n",
    "                a.append(new(a[2]))\n",
    "                if a[4] == '':\n",
    "                    a[4] = mo(a[3],a[2])\n",
    "        elif len(a[2]) == 10 and type(a[1]) == str:\n",
    "            if len(a[1]) == 6:\n",
    "                a.append(old(a[2],a[1]))\n",
    "            else:\n",
    "                print(row1.index(a))\n",
    "                mas.append(a[0])\n",
    "                row1.remove(a)\n",
    "              \n",
    "        else:\n",
    "            print(row1.index(a))\n",
    "            mas.append(a[0])\n",
    "            row1.remove(a)\n",
    "          "
