    "import pyodbc \n",
    "def zapros(pat,nom,sk):\n",
    "    zap = \"update DATA_INS_POLICIES_OMI set SMO = \"+str(sk)+\" where DATA313_ID = \"+str(pat)+\" and NOMER_POLISA = \"+str(nom)+\"\"\n",
    "    return zap\n",
    "connection_to_db = pyodbc.connect(r'driver={SQL server};Server=mediags-sql;Database=medialog_stac;Trusted_Connection=yes;')\n",
    "cursor=connection_to_db.cursor()\n",
    "for a in row1:\n",
    "    if len(a) == 5:\n",
    "        if a[4] != '':\n",
    "            try:\n",
    "                cursor.execute(zapros(a[0],a[2],a[4]))\n",
    "                connection_to_db.commit()\n",
    "            except:\n",
    "                print(a)\n",
    "connection_to_db.close()"
   ]
